/**index.wxss**/
.operate{
  margin: 15px 0;
}

.BelStatu {
  height: 4rpx;
  background-color: rgb(0, 206, 103);
}

.list {
  background-color: #ececec;
  margin: 20rpx 0 0;
  padding: 20rpx;
  border-radius: 10rpx;
}

.list .name {
  font-size: 18px;
  font-weight: bold;
}

.list .text {
  display: flex;
  font-size: 14px;
  margin-bottom: 10px;
}

.list .rssi,
.list .serverS {
  margin-right: 20rpx;
}

.list .data {
  word-break: break-all;  /* 换行 */
  font-size: 10px;
}

.link{
  text-align: right;
}

@import 'index-dark.wxss'